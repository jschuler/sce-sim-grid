{
  "name": "sce-sim-grid",
  "version": "0.3.0",
  "main": "dist/index",
  "module": "dist/index.js",
  "files": [
    "dist",
    "bundle"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/lib --out-dir dist --copy-files --extensions \".js,.ts,.tsx\" --source-maps inline --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:bundle": "rm -rf bundle && webpack --config webpack.config.js",
    "build-examples": "react-app-rewired build",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "compile": "./node_modules/typescript/bin/tsc -p ./tsconfig.build.json",
    "eject": "react-app-rewired eject",
    "start": "PORT=2000 react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom",
    "copy:css": "yarn copyfiles -u 2 src/lib/**/*.css dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "dependencies": {
    "@patternfly/patternfly": "^2.33.5",
    "@patternfly/react-core": "3.117.5",
    "@welldone-software/why-did-you-render": "^3.3.5",
    "classnames": "^2.2.6",
    "global": "^4.4.0",
    "jsonix": "^3.0.0",
    "react-infinite-scroll-component": "^4.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-infinite-scroll-component": "^4.2.4",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.2.0",
    "customize-cra": "^0.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "jsonix-schema-compiler": "^2.3.9",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.10.2",
    "react-scripts": "^3.2.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.0-dev.20191021",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.0.0-beta.1",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
